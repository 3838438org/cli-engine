{
  "name": "cli-engine",
  "description": "Generic CLI Framework",
  "version": "3.0.0",
  "author": "Jeff Dickey @dickeyxxx",
  "bin": {
    "cli-build": "bin/cli-build",
    "cli-release": "bin/cli-release"
  },
  "bugs": "https://github.com/heroku/cli-engine/issues",
  "dependencies": {
    "ansi-escapes": "1.4.0",
    "cli-engine-command": "3.0.1",
    "cli-engine-config": "1.0.2",
    "execa": "0.6.3",
    "find-node-modules": "^1.0.4",
    "fs-extra": "2.1.2",
    "klaw-sync": "1.1.2",
    "levenshtein": "^1.0.5",
    "lodash.maxby": "4.6.0",
    "lodash.uniqby": "^4.7.0",
    "log-chopper": "0.1.1",
    "moment": "2.18.1",
    "rwlockfile": "1.3.7",
    "string": "3.3.3",
    "tar-stream": "1.5.2",
    "yarn": "0.22.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "eslint-plugin-flowtype": "2.30.4",
    "flow-bin": "0.43.1",
    "flow-copy-source": "1.1.0",
    "flow-typed": "2.0.0",
    "jest": "19.0.2",
    "nock": "9.0.13",
    "rimraf": "2.6.1",
    "standard": "10.0.1"
  },
  "engines": {
    "node": ">= 7.0.0"
  },
  "enginestrict": true,
  "files": [
    "lib",
    "bin"
  ],
  "homepage": "https://github.com/heroku/cli-engine",
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "coveragePathIgnorePatterns": [
      "src/linewrap.js"
    ],
    "setupFiles": [
      "../test/init.js"
    ]
  },
  "keywords": [
    "cli"
  ],
  "license": "ISC",
  "main": "lib/cli.js",
  "repository": "https://github.com/heroku/cli-engine",
  "scripts": {
    "build": "babel src -d lib --ignore '*.test.js'",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "release": "np",
    "test": "jest && flow && standard",
    "watch": "babel --watch src -d lib --ignore '*.test.js'"
  },
  "standard": {
    "parser": "babel-eslint",
    "envs": [
      "jest"
    ],
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "Class",
      "jasmine",
      "stream$Readable"
    ],
    "ignore": [
      "src/linewrap.js",
      "example/node_modules",
      "example/tmp",
      "example-plugin",
      "flow-typed"
    ]
  }
}
