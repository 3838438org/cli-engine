{
  "name": "cli-engine",
  "description": "Generic CLI Framework",
  "version": "0.0.5",
  "author": "Jeff Dickey @dickeyxxx",
  "bin": {
    "cli-build": "bin/build",
    "cli-release": "bin/release"
  },
  "bugs": "https://github.com/heroku/cli-engine/issues",
  "dependencies": {
    "ansi-escapes": "1.4.0",
    "execa": "0.6.0",
    "find-up-sync": "1.0.2",
    "fs-extra": "2.0.0",
    "heroku-cli-command": "0.1.4",
    "klaw-sync": "1.1.1",
    "lodash.maxby": "4.6.0",
    "moment": "2.17.1",
    "rwlockfile": "1.3.3",
    "string": "3.3.3",
    "tar-stream": "1.5.2",
    "yarn": "0.20.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "cli-engine-example-plugin": "1.0.0",
    "heroku-cli-test": "0.0.2",
    "mocha": "3.2.0",
    "nyc": "10.1.2",
    "standard": "8.6.0",
    "time-require": "0.1.2"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 7.0.0"
  },
  "homepage": "https://github.com/heroku/cli-engine",
  "keywords": [
    "cli"
  ],
  "license": "ISC",
  "main": "cli.js",
  "repository": "https://github.com/heroku/cli-engine",
  "scripts": {
    "test": "nyc mocha --use_strict && standard"
  },
  "standard": {
    "ignore": [
      "lib/linewrap.js"
    ]
  },
  "cli-engine": {
    "plugins": [
      "cli-engine-example-plugin"
    ]
  }
}
